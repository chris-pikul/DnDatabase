{
  "name": "dndatabase",
  "version": "1.0.0",
  "description": "Dungeons and Dragons 5th Edition Compendium and tool set",
  "main": "index.js",
  "scripts": {
    "normalize": "cd compendium/scripts && node normalize all",
    "normalize:items": "cd compendium/scripts && node normalize items",
